		<input_parameter>
			<serial_no>1111</serial_no>
			<card_id>47</card_id>
			<user_id>1307891232</user_id>
			<partner>1</partner>
		</input_parameter>
		<expect>
			<code>Q00301</code>
		</expect>
	</testcase>
