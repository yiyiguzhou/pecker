	</testcase>
	<testcase id="10" name="参数card_id填写为非int型字符">
		<input_parameter>
			<card_id>3.7</card_id>
			<partner>1</partner>
			<serial_no>1111</serial_no>
			<user_id>1457298361</user_id>
