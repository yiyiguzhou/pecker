	<testcase id="19" name="重复请求">
		<input_parameter>
			<card_id>47</card_id>
			<partner>1</partner>
			<serial_no>1111</serial_no>
			<user_id>2004422136</user_id>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>Q00302</code>
		</expect>
	</testcase>
	<testcase id="20" name="所有参数正确">
		<input_parameter>
			<card_id>47</card_id>
			<partner>1</partner>
			<serial_no>1111</serial_no>
			<user_id>12694846</user_id>
