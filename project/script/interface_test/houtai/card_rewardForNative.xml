<?xml version="1.0" ?>
<suite shelf="card_rewardForNative">
	<url>
		<url_name>card_rewardForNative</url_name>
		<url_place>http://apisgame.qiyi.domain/user/card/rewardForNative?</url_place>
		<response_method>GET</response_method>
	</url>
	<testcase id="1" name="必填参数serial_no不上传">
		<input_parameter>
			<card_id>47</card_id>
			<sign>e39aeaa24e050364b5761c26c5c07a6e</sign>
			<user_id>1307891232</user_id>
			<partner>1</partner>
		</input_parameter>
		<expect>
			<code>Q00301</code>
		</expect>
	</testcase>
	<testcase id="2" name="必填参数card_id不上传">
		<input_parameter>
			<serial_no>1111</serial_no>
			<sign>d7c0b366bcfea54d74d3a6a131e57328</sign>
			<user_id>1307891232</user_id>
			<partner>1</partner>
		</input_parameter>
		<expect>
			<code>Q00301</code>
		</expect>
	</testcase>
	<testcase id="3" name="必填参数sign不上传">
		<input_parameter>
			<serial_no>1111</serial_no>
			<card_id>47</card_id>
			<user_id>1307891232</user_id>
			<partner>1</partner>
		</input_parameter>
		<expect>
			<code>Q00301</code>
		</expect>
	</testcase>
	<testcase id="4" name="必填参数user_id不上传">
		<input_parameter>
			<serial_no>1111</serial_no>
			<card_id>47</card_id>
			<sign>c4dcb2f117f19ba7125da193f91391d8</sign>
			<partner>1</partner>
		</input_parameter>
		<expect>
			<code>Q00301</code>
		</expect>
	</testcase>
	<testcase id="5" name="必填参数serial_no不填写">
		<input_parameter>
			<card_id>47</card_id>
			<sign>e39aeaa24e050364b5761c26c5c07a6e</sign>
			<user_id>1307891232</user_id>
			<partner>1</partner>
		</input_parameter>
		<expect>
			<code>Q00301</code>
		</expect>
	</testcase>
