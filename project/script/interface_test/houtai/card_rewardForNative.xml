		<input_parameter>
			<card_id>200</card_id>
			<partner>1</partner>
			<serial_no>1111</serial_no>
			<user_id>1466084297</user_id>
			<sign>d88c3752b961f7ff834f3b4d5201e7ee</sign>
		</input_parameter>
