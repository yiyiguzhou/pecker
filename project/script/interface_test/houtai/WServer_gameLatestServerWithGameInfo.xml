<?xml version="1.0" ?>
<suite shelf="WServer_gameLatestServerWithGameInfo">
	<url>
		<url_name>WServer_gameLatestServerWithGameInfo</url_name>
		<url_place>http://apisgame.qiyi.domain/product/web/WServer/gameLatestServerWithGameInfo?</url_place>
		<response_method>GET</response_method>
	</url>
	<testcase id="1" name="传入单个游戏id&amp;返回游戏信息和该游戏最新服务器信息">
		<input_parameter>
			<game_id>4882</game_id>
			<source>1</source>
			<sign>7fff9ab29d5aa2d8bd0989765d6abe7f</sign>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<game_id_4882>
					<game>
						<name>盗墓笔记</name>
					</game>
					<server>
						<id test_type="exist">4882</id>
						<order test_type="exist">770</order>
					</server>
				</game_id_4882>
			</data>
		</expect>
	</testcase>
