<?xml version="1.0" ?>
<suite shelf="user_userPlayedGame">
	<url>
		<url_name>user_userPlayedGame</url_name>
		<url_place>http://apisgame.qiyi.domain/user/user/userPlayedGame?</url_place>
		<response_method>GET</response_method>
	</url>
	<testcase id="1" name="source为空或不上传">
		<input_parameter>
			<game_id>1234</game_id>
			<limit>5</limit>
			<sign>017187fe420bdbb5bdd03cfe65e0b982</sign>
			<terminal>1</terminal>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="2" name="source错误">
		<input_parameter>
			<game_id>1234</game_id>
			<limit>5</limit>
			<sign>017187fe420bdbb5bdd03cfe65e0b982</sign>
			<source>a</source>
			<terminal>1</terminal>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="3" name="sign为空或不上传">
		<input_parameter>
			<game_id>1234</game_id>
			<limit>5</limit>
			<source>6</source>
			<terminal>1</terminal>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="4" name="sign错误">
		<input_parameter>
			<game_id>1234</game_id>
			<limit>5</limit>
			<sign>017187fe420bdbb5bdd03cfe65e0b9821</sign>
			<source>6</source>
			<terminal>1</terminal>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10002</code>
		</expect>
	</testcase>
	<testcase id="5" name="uid为空或不上传">
		<input_parameter>
			<game_id>1234</game_id>
			<limit>5</limit>
			<sign>4bfb61677b4a5102ddb769242e6adb6e</sign>
			<source>6</source>
			<terminal>1</terminal>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="6" name="uid格式错误">
		<input_parameter>
			<game_id>1234</game_id>
			<limit>5</limit>
			<sign>c51aa8337d70e8f982172632b08937ca</sign>
			<source>6</source>
			<terminal>1</terminal>
			<uid>14a752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="7" name="uid格式正确但是用户不存在">
		<input_parameter>
			<sign>9275b5b46faa8795e4cd604da118cd67</sign>
			<source>6</source>
			<uid>1333243423</uid>
		</input_parameter>
		<expect>
			<code>200</code>
		</expect>
	</testcase>
	<testcase id="8" name="terminal为空不上传">
		<input_parameter>
			<sign>c48a45dbb137bc7a94c6517912cccb12</sign>
			<source>6</source>
			<uid>1307891232</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<game_id>5786</game_id>
			</data>
		</expect>
	</testcase>
	<testcase id="9" name="terminal上传(1-7)">
		<input_parameter>
			<sign>bc9c9d380e9855a8ea1736c30be07f86</sign>
			<source>6</source>
			<terminal>4</terminal>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<game_id>6630</game_id>
				<qipu_id>213907420</qipu_id>
				<game_info>
					<game_id>6630</game_id>
					<qipu_id>213907420</qipu_id>
					<game_name>蜀山外传</game_name>
				</game_info>
			</data>
			<data>
				<game_id>6214</game_id>
				<qipu_id>212795420</qipu_id>
				<game_info>
					<game_id>6214</game_id>
					<qipu_id>212795420</qipu_id>
					<game_name>玩个球</game_name>
				</game_info>
			</data>
			<data>
				<game_id>4882</game_id>
				<qipu_id>210478920</qipu_id>
				<game_info>
					<game_id>4882</game_id>
					<qipu_id>210478920</qipu_id>
					<game_name>盗墓笔记</game_name>
				</game_info>
			</data>
		</expect>
	</testcase>
	<testcase id="10" name="terminal上传错误">
		<input_parameter>
			<sign>2e3b69950ddfc5621870dc1dbe27d28b</sign>
			<source>6</source>
			<terminal>9</terminal>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10013</code>
		</expect>
	</testcase>
	<testcase id="11" name="limit不传为空">
		<input_parameter>
			<sign>51e700f6ed1d8cc6e9ecda88576ac56f</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<game_id>10003</game_id>
				<qipu_id>207704720</qipu_id>
				<terminal>1</terminal>
				<server_id>0</server_id>
			</data>
		</expect>
	</testcase>
	<testcase id="12" name="limit&gt;60">
		<input_parameter>
			<limit>61</limit>
			<sign>669e006542d88c25238f277176743039</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10012</code>
		</expect>
	</testcase>
	<testcase id="13" name="limit不为int">
		<input_parameter>
			<limit>a</limit>
			<sign>c9a631b443b2c70260d56519eb54a6a5</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="14" name="game_id为空不上传">
		<input_parameter>
			<sign>51e700f6ed1d8cc6e9ecda88576ac56f</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<game_id>10003</game_id>
				<qipu_id>207704720</qipu_id>
				<terminal>1</terminal>
				<server_id>0</server_id>
			</data>
		</expect>
	</testcase>
	<testcase id="15" name="game_id格式错误">
		<input_parameter>
			<game_id>1d34</game_id>
			<sign>be9dacae35d83902e12c9ef732c44dbb</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="16" name="game_id不存在">
		<input_parameter>
			<game_id>1234</game_id>
			<sign>441fe234a8b05f5d528b8b4e345e763e</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>200</code>
		</expect>
	</testcase>
	<testcase id="17" name="game_id存在">
		<input_parameter>
			<game_id>6519</game_id>
			<sign>14ac87bc05d2caa695585320209395f2</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<game_id>6519</game_id>
				<qipu_id>212801620</qipu_id>
				<terminal>6</terminal>
				<server_id>0</server_id>
				<togame_time>2017-10-1011:44:57</togame_time>
			</data>
		</expect>
	</testcase>
	<testcase id="18" name="只填必填参数">
		<input_parameter>
			<sign>51e700f6ed1d8cc6e9ecda88576ac56f</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<game_id>10003</game_id>
			</data>
		</expect>
	</testcase>
</suite>
