<?xml version="1.0" ?>
<suite shelf="qibei_freeze">
	<url>
		<url_name>qibei_freeze</url_name>
		<url_place>http://apisgame.qiyi.domain/user/qibei/freeze?</url_place>
		<response_method>POST</response_method>
	</url>
	<testcase id="1" name="请求成功">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>2</channel_detail>
			<frozen_time>300</frozen_time>
			<number>1</number>
			<sign>0c6dde6b70384a7acceef3490622ef11</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
			<desc>kk</desc>
		</input_parameter>
		<expect>
			<code>200</code>
		</expect>
	</testcase>
	<testcase id="2" name="source不传">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1708311357000019522</channel_detail>
			<frozen_time>300</frozen_time>
			<number>20</number>
			<sign>f1aea8add97376dac328739473487075</sign>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="3" name="source格式错误">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1708311357000019522</channel_detail>
			<frozen_time>300</frozen_time>
			<number>20</number>
			<sign>f1aea8add97376dac328739473487075</sign>
			<source>a</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="4" name="source格式正确，不在范围内">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1708311357000019522</channel_detail>
			<frozen_time>300</frozen_time>
			<number>20</number>
			<sign>f1aea8add97376dac328739473487075</sign>
			<source>66</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="5" name="sign为空">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1708311357000019522</channel_detail>
			<frozen_time>300</frozen_time>
			<number>20</number>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="6" name="sign错误">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1708311357000019522</channel_detail>
			<frozen_time>300</frozen_time>
			<number>20</number>
			<sign>111</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10002</code>
		</expect>
	</testcase>
