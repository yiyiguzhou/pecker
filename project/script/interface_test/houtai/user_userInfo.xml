<?xml version="1.0" ?>
<suite shelf="user_userInfo">
	<url>
		<url_name>user_userInfo</url_name>
		<url_place>http://10.10.31.1/user/user/userInfo?</url_place>
		<response_method>GET</response_method>
	</url>
	<testcase id="1" name="少传uid或authcoockie">
		<input_parameter>
			<sign>%{sign}</sign>
			<source>6</source>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="2" name="source和sign均未传">
		<input_parameter>
			<authcookie>e4CazEXcFBhXSRRr5HiDihkJQv4eXm1E1oNm1m1rZFPKU74um2pLM07G1FTS7joyyGiySN65</authcookie>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="3" name="source不传">
		<input_parameter>
			<authcookie>f9EBPQy1Ay3vrv6WXMkpQcEIgEoDsvm2VWPisgxwbegWwiZIoGlxrm3FlBk7J2MSIRLK0c</authcookie>
			<sign>%{sign}</sign>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="4" name="source不在范围内">
		<input_parameter>
			<authcookie>f9EBPQy1Ay3vrv6WXMkpQcEIgEoDsvm2VWPisgxwbegWwiZIoGlxrm3FlBk7J2MSIRLK0c</authcookie>
			<sign>%{sign}</sign>
			<source>66</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="5" name="source不为int">
		<input_parameter>
			<authcookie>f9EBPQy1Ay3vrv6WXMkpQcEIgEoDsvm2VWPisgxwbegWwiZIoGlxrm3FlBk7J2MSIRLK0c</authcookie>
			<sign>%{sign}</sign>
			<source>aa</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="6" name="sign不传">
		<input_parameter>
			<authcookie>f9EBPQy1Ay3vrv6WXMkpQcEIgEoDsvm2VWPisgxwbegWwiZIoGlxrm3FlBk7J2MSIRLK0c</authcookie>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="7" name="sign错误">
		<input_parameter>
			<authcookie>f9EBPQy1Ay3vrv6WXMkpQcEIgEoDsvm2VWPisgxwbegWwiZIoGlxrm3FlBk7J2MSIRLK0c</authcookie>
			<sign>c8e1067629b7f868a30d90f0803a943b</sign>
			<source>6</source>
			<uid>1444516752</uid>
		</input_parameter>
		<expect>
			<code>10002</code>
		</expect>
	</testcase>
	<testcase id="8" name="只上传authcookie&amp;其余正确传">
		<input_parameter>
			<authcookie>8d0pk3idbWoiNxrRp1uU6N1vxFDvqMq0pn5UIloO1txUde2y229m3go9e9Rr535Xtm2U5c</authcookie>
			<sign>d96189d6e90eef750f59c58fcdc8b7c4</sign>
			<source>6</source>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<uid>1422887039</uid>
				<suid>0</suid>
			</data>
		</expect>
	</testcase>
	<testcase id="9" name="authcookie不存在">
		<input_parameter>
			<authcookie>f9EBPQy1Ay3vrv6WXMkpQcEIgEoDsvm2VWPisgxwbegWwiZIoGlxrm3FlBk7J2MSIRLK0c</authcookie>
			<source>6</source>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10008</code>
		</expect>
	</testcase>
	<testcase id="10" name="只上传uid&amp;其余正确">
		<input_parameter>
			<sign>%{sign}</sign>
			<source>6</source>
			<uid>1164371952</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<uid>1164371952</uid>
				<suid>1164371952</suid>
			</data>
		</expect>
	</testcase>
	<testcase id="11" name="uid不存在">
		<input_parameter>
			<sign>%{sign}</sign>
			<source>6</source>
			<uid>116437195233333333</uid>
		</input_parameter>
		<expect>
			<code>10008</code>
		</expect>
	</testcase>
	<testcase id="12" name="authcookie和uid都上传正确">
		<input_parameter>
			<authcookie>e4CazEXcFBhXSRRr5HiDihkJQv4eXm1E1oNm1m1rZFPKU74um2pLM07G1FTS7joyyGiySN65</authcookie>
			<sign>%{sign}</sign>
			<source>6</source>
			<uid>1164371952</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<uid>1164371952</uid>
				<suid>1164371952</suid>
			</data>
		</expect>
	</testcase>
	<testcase id="13" name="authcookie错误uid正确(以uid为准)">
		<input_parameter>
			<authcookie>f9EBPQwbegWwiZIoGlxrm3FlBk7J2MSIRLK0c</authcookie>
			<sign>%{sign}</sign>
			<source>6</source>
			<uid>1164371952</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<uid>1164371952</uid>
				<suid>1164371952</suid>
			</data>
		</expect>
	</testcase>
	<testcase id="14" name="uid错误authcookie正确">
		<input_parameter>
			<authcookie>e4CazEXcFBhXSRRr5HiDihkJQv4eXm1E1oNm1m1rZFPKU74um2pLM07G1FTS7joyyGiySN65</authcookie>
			<sign>%{sign}</sign>
			<source>6</source>
			<uid>116437195233333333</uid>
		</input_parameter>
		<expect>
			<code>10008</code>
		</expect>
	</testcase>
	<testcase id="15" name="authcookie uid都错误">
		<input_parameter>
			<authcookie>m1m1rZFPKU74um2pLM07G1FTS7joyyGiySN65</authcookie>
			<sign>%{sign}</sign>
			<source>6</source>
			<uid>116437195233333333</uid>
		</input_parameter>
		<expect>
			<code>10008</code>
		</expect>
	</testcase>
	<testcase id="16" name="用户非会员">
		<input_parameter>
			<sign>e767d0c702200ba25ba7b4804bba86b2</sign>
			<source>6</source>
			<uid>35435752</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<uid>35435752</uid>
				<suid>35435752</suid>
			</data>
		</expect>
	</testcase>
	<testcase id="17" name="用户体验会员">
		<input_parameter>
			<sign>f578cd6aee170ff34d39a673f4a5675a</sign>
			<source>6</source>
			<uid>1415698674</uid>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<uid>1415698674</uid>
				<suid>1415698674</suid>
			</data>
		</expect>
	</testcase>
	<testcase id="18" name="用户为充值会员">
		<input_parameter>
			<source>6</source>
			<uid>1466090552</uid>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<uid>1466090552</uid>
				<suid>1466090552</suid>
			</data>
		</expect>
	</testcase>
</suite>
