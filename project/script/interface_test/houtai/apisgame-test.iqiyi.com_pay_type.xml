<?xml version="1.0" ?>
<suite shelf="apisgame-test.iqiyi.com_pay_type">
	<url>
		<url_name>apisgame-test.iqiyi.com_pay_type</url_name>
		<url_place>https://apisgame-test.iqiyi.com/pay_type?</url_place>
		<response_method>GET</response_method>
	</url>
	<testcase id="1" name="必填参数source不传">
		<input_parameter>
			<game_id>6974</game_id>
			<qudao_terminal>1</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="2" name="source不在枚举值范围内">
		<input_parameter>
			<source>88</source>
			<game_id>10044</game_id>
			<qudao_terminal>1</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="3" name="必填参数qudao_terminal不传">
		<input_parameter>
			<source>2</source>
			<game_id>10044</game_id>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="4" name="qudao_terminal不在枚举值范围内">
		<input_parameter>
			<source>2</source>
			<game_id>6974</game_id>
			<qudao_terminal>9</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<gameInfo>
					<pay_status>3</pay_status>
					<game_coin_name>w</game_coin_name>
					<exchange_rate>2</exchange_rate>
				</gameInfo>
			</data>
		</expect>
	</testcase>
	<testcase id="5" name="必填参数game_id不传">
		<input_parameter>
			<source>2</source>
			<qudao_terminal>1</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="6" name="game_id传入的格式错误">
		<input_parameter>
			<source>2</source>
			<game_id>ww</game_id>
			<qudao_terminal>1</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="7" name="必传参数sign未传">
		<input_parameter>
			<source>2</source>
			<game_id>6974</game_id>
			<qudao_terminal>1</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="8" name="sign传错">
		<input_parameter>
			<source>2</source>
			<game_id>6974</game_id>
			<qudao_terminal>1</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>22</sign>
		</input_parameter>
		<expect>
			<code>10002</code>
		</expect>
	</testcase>
	<testcase id="9" name="必传参数timestamp未传">
		<input_parameter>
			<source>2</source>
			<game_id>6974</game_id>
			<qudao_terminal>1</qudao_terminal>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="10" name="timestamp格式错误">
		<input_parameter>
			<source>2</source>
			<game_id>6974</game_id>
			<qudao_terminal>1</qudao_terminal>
			<timestamp>hhh</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="11" name="接口请求成功">
		<input_parameter>
			<source>2</source>
			<game_id>6974</game_id>
			<qudao_terminal>4</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<gameInfo>
					<pay_status>3</pay_status>
					<game_coin_name>w</game_coin_name>
					<exchange_rate>2</exchange_rate>
				</gameInfo>
				<qudaoList>
					<qudao_id>8254</qudao_id>
				</qudaoList>
				<qudaoList>
					<qudao_id>8253</qudao_id>
				</qudaoList>
				<qudaoList>
					<qudao_id>8004</qudao_id>
				</qudaoList>
				<qudaoList>
					<qudao_id>8001</qudao_id>
				</qudaoList>
				<qudaoList>
					<qudao_id>8252</qudao_id>
				</qudaoList>
			</data>
		</expect>
	</testcase>
	<testcase id="12" name="获取渠道列表失败">
		<input_parameter>
			<source>2</source>
			<game_id>10014</game_id>
			<qudao_terminal>4</qudao_terminal>
			<timestamp>%{timestamp}</timestamp>
			<sign>%{sign}</sign>
		</input_parameter>
		<expect>
			<code>30200</code>
		</expect>
	</testcase>
</suite>
