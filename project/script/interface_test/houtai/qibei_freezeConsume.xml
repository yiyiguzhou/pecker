<?xml version="1.0" ?>
<suite shelf="qibei_freezeConsume">
	<url>
		<url_name>qibei_freezeConsume</url_name>
		<url_place>http://apisgame.qiyi.domain/user/qibei/freezeConsume?</url_place>
		<response_method>POST</response_method>
	</url>
	<testcase id="1" name="内网访问">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1712181021000072422</channel_detail>
			<sign>0a7e0f4152bbfa33cd28f9c141d344e0</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>200</code>
		</expect>
	</testcase>
	<testcase id="2" name="外网访问">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1712181021000072422</channel_detail>
			<sign>0a7e0f4152bbfa33cd28f9c141d344e0</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>200</code>
		</expect>
	</testcase>
	<testcase id="3" name="source不传、为空">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>124345</channel_detail>
			<sign>3e7d2fcee4134c4cc03810f4fcdf7314</sign>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="4" name="source错误">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>124345</channel_detail>
			<sign>3e7d2fcee4134c4cc03810f4fcdf7314</sign>
			<source>66</source>
			<user_id>144516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="5" name="sign不传、为空">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>124345</channel_detail>
			<source>66</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="6" name="sign错误">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>124345</channel_detail>
			<sign>3e7d2fcee4134c4cc03810</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10002</code>
		</expect>
	</testcase>
	<testcase id="7" name="user_id不传、为空">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>124345</channel_detail>
			<sign>1488705e6801cf2f5d48f64cd572b205</sign>
			<source>6</source>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="8" name="user_id格式错误">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>124345</channel_detail>
			<sign>28d4499dd60bda0e23a8688cd403e368</sign>
			<source>6</source>
			<user_id>a516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="9" name="user_id不存在">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1712181422000073422</channel_detail>
			<sign>7b6fa768166a11e62402ff69faf9c111</sign>
			<source>6</source>
			<user_id>12312312</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="10" name="channel不传、为空">
		<input_parameter>
			<channel_detail>1712181021000072422</channel_detail>
			<sign>1670064197ec47b46874316b25923960</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="11" name="channel格式错误">
		<input_parameter>
			<channel>a</channel>
			<channel_detail>1712181021000072422</channel_detail>
			<sign>fc1d71d23d4a3630544669f80da69996</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="12" name="channel≠4">
		<input_parameter>
			<channel>22</channel>
			<channel_detail>1712181021000072422</channel_detail>
			<sign>b37de92d596d7622624881674a321e7f</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="13" name="channel_detail不传、为空">
		<input_parameter>
			<channel>22</channel>
			<sign>027e411411623995bc57db9d08c8409e</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="14" name="channel_detail格式错误">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>a172422</channel_detail>
			<sign>79e9a43f3d3727000e25594ada71fa3c</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="15" name="channel_detail不存在">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1712181021000000000</channel_detail>
			<sign>2d4fedcb3025762d2c6102ea5c16831c</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>30007</code>
		</expect>
	</testcase>
	<testcase id="16" name="上传channel_detail不为买断的订单id">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1709222032000069622</channel_detail>
			<sign>bb8894de0c558f6db0e53bb4c848eb61</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>30007</code>
		</expect>
	</testcase>
	<testcase id="17" name="所有参数正确上传">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1712181021000072422</channel_detail>
			<sign>0a7e0f4152bbfa33cd28f9c141d344e0</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>200</code>
		</expect>
	</testcase>
	<testcase id="18" name="没有冻结记录">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1712181020000072322</channel_detail>
			<sign>f7135ed5b74ca2a2dd1b4030a340b909</sign>
			<source>6</source>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>30007</code>
		</expect>
	</testcase>
	<testcase id="19" name="奇贝冻结数量异常">
		<input_parameter>
			<channel>4</channel>
			<channel_detail>1709291817000073622</channel_detail>
			<sign>a514ed00556eff123eb4671db1704745</sign>
			<source>6</source>
			<user_id>1162975387</user_id>
		</input_parameter>
		<expect>
			<code>30003</code>
		</expect>
	</testcase>
</suite>
