<?xml version="1.0" ?>
<suite shelf="product_cancelOrder">
	<url>
		<url_name>product_cancelOrder</url_name>
		<url_place>https://apisgame-test.iqiyi.com/pay/product/cancelOrder?</url_place>
		<response_method>POST</response_method>
	</url>
	<testcase id="1" name="必填参数source不上传">
		<input_parameter>
			<order_id>1801021737000010722</order_id>
			<sign>%{sign</sign>
			<timestamp>%{timestamp}</timestamp>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="2" name="必填参数sign不上传">
		<input_parameter>
			<order_id>1801021737000010722</order_id>
			<source>1</source>
			<timestamp>%{timestamp}</timestamp>
			<user_id>1444516752</user_id>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
