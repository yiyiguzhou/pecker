	</testcase>
	<testcase id="4" name="用户买过的游戏_传入多个游戏id，id之间用英文状态下除逗号外其他符号隔开">
		<input_parameter>
			<source>6</source>
			<sign>240e111920438cd63ef31a6910d3d9ae</sign>
			<uid>1466090552</uid>
			<game_ids>6587*6582</game_ids>
		</input_parameter>
		<expect>
			<code>200</code>
			<data>
				<id>3452</id>
				<uid>1466090552</uid>
				<product_id>6587</product_id>
				<type>1</type>
			</data>
		</expect>
	</testcase>
