<?xml version="1.0" ?>
<suite shelf="user_getUserBoughtGame">
	<url>
		<url_name>user_getUserBoughtGame</url_name>
		<url_place>http://apisgame.qiyi.domain/user/user/getUserBoughtGame?</url_place>
		<response_method>GET</response_method>
	</url>
	<testcase id="1" name="用户买过的游戏_参数用户ID不填">
		<input_parameter>
			<source>6</source>
			<sign>336e16f10be8d4b9b25776663cde9c34</sign>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="2" name="用户买过的游戏_参数source不填">
		<input_parameter>
			<sign>f98686509e7d6803346a77a271444d86</sign>
			<uid>1466090552</uid>
		</input_parameter>
		<expect>
			<code>10001</code>
		</expect>
	</testcase>
	<testcase id="3" name="用户买过的游戏_传入参数签名不填">
